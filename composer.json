{
    "name": "generahben/laravel-remita",
    "description": "A Laravel package to conveniently integrate Remita payment gateway",
    "type": "library",
    "keywords": [
        "php",
        "github",
        "laravel",
        "Open Source",
        "payments",
        "remita payment gateway",
        "subscription",
        "remita",
        "remita.net",
        "laravel 9"
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7",
        "illuminate/support": "^9"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Generahben\\Remita\\": "src/",
            "Generahben\\Remita\\Test\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Generah Ben",
            "email": "generahben@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Generahben\\Remita\\Providers\\RemitaServiceProvider"
            ],
            "aliases": {
                "Remita": "Generahben\\Remita\\Facades\\Remita"
            }
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^7.7"
    }
}
